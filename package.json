{
  "name": "JobTracker",
  "productName": "JobTracker-alpha",
  "version": "0.5.2",
  "description": "An easy-to-use and stylish Job tracking application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "rebuild": "",
    "rebuild-sqlite3": "npm run configure-sqlite3 && npm run build-sqlite3",
    "configure-sqlite3": "cd node_modules/sqlite3 && npm run prepublish && node-gyp configure --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-win32-x64",
    "build-sqlite3": "cd node_modules/sqlite3 && node-gyp rebuild --target=1.4.15 --arch=x64 --target_platform=win32 --dist-url=https://atom.io/download/atom-shell --module_name=node_sqlite3 --module_path=../lib/binding/electron-v1.4-win32-x64"
  },
  "keywords": [
    "JobTracker",
    "Electron"
  ],
  "author": "Vasilis Mourtzis",
  "license": "MIT 1.0",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "datejs": "^1.0.0-rc3",
    "docxtemplater": "^3.0.3",
    "eonasdan-bootstrap-datetimepicker-npm": "^4.17.37",
    "jquery": "2.2",
    "jsrender": "^0.9.83",
    "jszip": "^2.6.1",
    "less": "2.7.1",
    "moment": "^2.14.1",
    "nan": "^2.5.1",
    "sequelize": "^3.30.1",
    "sqlite": "^2.3.0"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "1.4.15",
    "electron-debug": "^1.1.0",
    "spectron": "^3.2.3"
  }
}
